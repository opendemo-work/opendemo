{
  "name": "basic-rag-platform",
  "version": "1.0.0",
  "description": "RAG平台基础部署",
  "author": "opendemo",
  "difficulty": "intermediate",
  "category": "rag",
  "tags": ["rag", "llm", "retrieval", "kubernetes"],
  "kubernetes_version": "v1.23+",
  "created_at": "2026-01-21",
